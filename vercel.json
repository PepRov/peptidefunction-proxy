{
  "version": 2,
  "builds": [
    {
      "src": "proxy.py",
      "use": "@vercel/python@1.1.0"
    }
  ],
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "proxy.py"
    },
    {
      "src": "/predict",
      "methods": ["POST"],
      "dest": "proxy.py"
    }
  ]
}



